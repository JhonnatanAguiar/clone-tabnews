{
  "name": "clone-tabnews",
  "version": "1.0.0",
  "description": "Projeto Tabnews do Curso.dev com Filipe Deschamps.",
  "main": "index.js",
  "scripts": {
    "dev": " npm run services:up && next dev",
    "services:up": "docker compose -f infra/compose.yaml up -d",
    "services:stop": "docker compose -f infra/compose.yaml stop",
    "services:down": "docker compose -f infra/compose.yaml down",
    "lint:check": "prettier --check .",
    "lint:fix": "prettier --write .",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "clean-port": "npx kill-port 3000"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "next": "^13.1.6",
    "pg": "^8.11.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "jest": "^29.6.2",
    "kill-port": "^2.0.1",
    "prettier": "^3.6.2"
  }
}
